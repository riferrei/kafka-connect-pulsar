{
    "name" : "multi-topic-with-regex",
    "config" : {
      "connector.class" : "com.riferrei.kafka.connect.pulsar.PulsarSourceConnector",
      "service.url": "pulsar://pulsar:6650",
      "service.http.url": "http://pulsar:8080",
      "topic.regex": "persistent://public/default/.*",
      "topic.poll.interval.ms": "5000",
      "tasks.max" : "3"
    }
}
